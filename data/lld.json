[
  {
    "id": 0,
    "title": "0. Runtime Anatomy",
    "subtitle": "The ABC",
    "groups": [
      {
        "name": "",
        "topics": [
          {"title": "The Anatomy of a Line", "priority": "high", "note": "Complete execution flow: Program Counter ticks -> Stack Frame carves 'Left' -> Heap allocates 'Right' -> Reference connects them." },
          {"title": "Identity vs Equality", "priority": "high", "note": "== vs .equals(). Comparing memory addresses vs. comparing actual data state." },
          {"title": "Object Duplication", "priority": "high", "note": "Shallow vs Deep Copy. Understanding pointer replication vs recursive memory allocation." },
          {"title": "The HashCode Contract", "priority": "medium", "note": "Why equality logic must match bucket logic for collections." }
        ]
      }
    ]
  },
  {
    "id": 1,
    "title": "1. OOP Physics",
    "subtitle": "The Foundation",
    "groups": [
      {
        "name": "Core Concepts",
        "topics": [
          { "title": "Classes & Objects", "priority": "high", "note": "The atom of OOP. Know memory layout." },
          { "title": "Interfaces", "priority": "high", "note": "Contracts over implementation. Essential for decoupling." },
          { "title": "Inheritance", "priority": "medium", "note": "Don't overuse. Can lead to tight coupling." },
          { "title": "Polymorphism", "priority": "high", "note": "Runtime magic. The basis of all flexible code." },
          { "title": "Abstraction", "priority": "high", "note": "Hiding complexity. API design 101." },
          { "title": "Encapsulation", "priority": "high", "note": "Protecting state. Getters/Setters are just the start." },
          { "title": "Access Modifiers", "priority": "medium", "note": "Package-private vs Protected plays a big role in library design." },
          { "title": "Object Lifecycle", "priority": "low", "note": "Creation to Garbage Collection." },
          { "title": "Stack vs Heap", "priority": "high", "note": "Crucial for debugging OOM errors and performance." }
        ]
      },
      {
        "name": "Relationships",
        "topics": [
          { "title": "Association", "priority": "low", "note": "General link between objects." },
          { "title": "Aggregation", "priority": "low", "note": "Has-a relationship (weak)." },
          { "title": "Composition", "priority": "high", "note": "Has-a relationship (strong). Owns the lifecycle." },
          { "title": "Composition over Inheritance", "priority": "high", "note": "The golden rule of modern OOD. Flexible & testable." }
        ]
      },
      {
        "name": "Mechanics",
        "topics": [
          { "title": "Diamond Problem", "priority": "medium", "note": "Why Java doesn't have multiple class inheritance." },
          { "title": "Dynamic Dispatch", "priority": "low", "note": "How the compiler decides which method to call." },
          { "title": "Immutability", "priority": "high", "note": "Thread-safe by default. Key for caching and DTOs." },
          { "title": "Abstract Class vs Interface", "priority": "high", "note": "Classic interview question. Know the 'is-a' vs 'can-do' distinction." }
        ]
      }
    ]
  },
  {
    "id": 2,
    "title": "2. UML Blueprint",
    "subtitle": "Visualizing Code",
    "groups": [
      {
        "name": "Structural",
        "topics": [
          { "title": "Class Diagram", "priority": "high", "note": "You will draw this in every LLD interview." },
          { "title": "UML Modifiers", "priority": "low", "note": "+, -, #. Just know the basics." },
          { "title": "Cardinality", "priority": "medium", "note": "1:1, 1:N. Important for DB schema mapping." }
        ]
      },
      {
        "name": "Behavioral",
        "topics": [
          { "title": "Use Case", "priority": "medium", "note": "Defining scope and actors." },
          { "title": "Sequence Diagram", "priority": "high", "note": "Vital for showing API flows and async processes." },
          { "title": "Activity Diagram", "priority": "low", "note": "Flowcharts for algorithms." },
          { "title": "State Machine", "priority": "medium", "note": "Crucial for payment flows and order management." },
          { "title": "System Boundary", "priority": "low", "note": "What is inside vs outside your app." },
          { "title": "Sync vs Async", "priority": "high", "note": "Blocking vs Non-blocking communication." }
        ]
      }
    ]
  },
  {
    "id": 3,
    "title": "3. Design Principles",
    "subtitle": "The Rules",
    "groups": [
      {
        "name": "SOLID",
        "topics": [
          { "title": "SRP", "priority": "high", "note": "Single Responsibility. One reason to change." },
          { "title": "Open/Closed", "priority": "high", "note": "Extend functionality without touching existing code." },
          { "title": "Liskov Substitution", "priority": "medium", "note": "Subtypes must behave like parent types." },
          { "title": "Interface Segregation", "priority": "high", "note": "No fat interfaces. Clients shouldn't depend on what they don't use." },
          { "title": "Dependency Inversion", "priority": "high", "note": "Depend on abstractions, not concretions. Basis of DI frameworks." }
        ]
      },
      {
        "name": "Heuristics",
        "topics": [
          { "title": "DRY", "priority": "medium", "note": "Don't Repeat Yourself." },
          { "title": "KISS", "priority": "medium", "note": "Keep It Simple, Stupid. Complexity is the enemy." },
          { "title": "YAGNI", "priority": "medium", "note": "You Ain't Gonna Need It. Avoid over-engineering." },
          { "title": "Law of Demeter", "priority": "low", "note": "Don't talk to strangers. a.getB().getC().do() is bad." },
          { "title": "Fail-Fast", "priority": "high", "note": "Validate inputs early. Throw exceptions immediately." },
          { "title": "Method Contracts", "priority": "low", "note": "Preconditions and Postconditions." }
        ]
      }
    ]
  },
  {
    "id": 4,
    "title": "4. Design Patterns",
    "subtitle": "The Toolkit",
    "groups": [
      {
        "name": "Creational",
        "topics": [
          { "title": "Singleton", "priority": "high", "note": "DB Connections, Config Loaders. Watch out for thread safety." },
          { "title": "Factory Method", "priority": "high", "note": "Decouples creation logic. heavily used in libraries." },
          { "title": "Builder", "priority": "high", "note": "Essential for complex objects (e.g., HTTP Requests)." }
        ]
      },
      {
        "name": "Structural",
        "topics": [
          { "title": "Adapter", "priority": "high", "note": "Connecting incompatible interfaces (Legacy code)." },
          { "title": "Facade", "priority": "medium", "note": "Simple interface to a complex subsystem." },
          { "title": "Decorator", "priority": "medium", "note": "Adding wrappers (e.g., I/O Streams, UI components)." },
          { "title": "Composite", "priority": "low", "note": "Tree structures (File systems, UI trees)." },
          { "title": "Proxy", "priority": "high", "note": "Lazy loading, Access control, Spring AOP." }
        ]
      },
      {
        "name": "Behavioral",
        "topics": [
          { "title": "Strategy", "priority": "high", "note": "Swappable algorithms (Payment methods, Sort strategies)." },
          { "title": "Observer", "priority": "high", "note": "Event listeners, Pub/Sub mechanisms." },
          { "title": "Command", "priority": "medium", "note": "Encapsulate requests (Undo/Redo logic)." },
          { "title": "State", "priority": "medium", "note": "Managing object states (Order processing)." },
          { "title": "Template Method", "priority": "medium", "note": "Skeleton algorithms. Hooks for subclasses." },
          { "title": "Chain of Responsibility", "priority": "medium", "note": "Middleware pipelines, Logging filters." },
          { "title": "Iterator", "priority": "low", "note": "Traversing collections without exposing internals." }
        ]
      }
    ]
  },
  {
    "id": 5,
    "title": "5. Data Modeling",
    "subtitle": "Persistence",
    "groups": [
      {
        "name": "Design",
        "topics": [
          { "title": "ER Diagrams", "priority": "high", "note": "Visualizing DB tables and relations." },
          { "title": "Normalization", "priority": "high", "note": "Removing redundancy (1NF, 2NF, 3NF)." },
          { "title": "Entity vs Value Object", "priority": "medium", "note": "Identity vs Attributes. DDD concept." }
        ]
      },
      {
        "name": "ORM",
        "topics": [
          { "title": "ORM Mapping", "priority": "medium", "note": "Hibernate/JPA annotations." },
          { "title": "Lazy vs Eager", "priority": "high", "note": "N+1 Problem causes major perf issues." },
          { "title": "Schema Evolution", "priority": "low", "note": "Handling DB migrations (Liquibase/Flyway)." }
        ]
      }
    ]
  },
  {
    "id": 6,
    "title": "6. Concurrency",
    "subtitle": "Multithreading",
    "groups": [
      {
        "name": "Basics",
        "topics": [
          { "title": "Threads vs Processes", "priority": "medium", "note": "Memory sharing differences." },
          { "title": "Thread Lifecycle", "priority": "low", "note": "New, Runnable, Blocked, Waiting." }
        ]
      },
      {
        "name": "Sync",
        "topics": [
          { "title": "Locks", "priority": "high", "note": "Synchronized blocks, Deadlocks." },
          { "title": "ReentrantLock", "priority": "medium", "note": "Advanced locking with fairness policies." },
          { "title": "Volatile", "priority": "medium", "note": "Visibility guarantee, prevents instruction reordering." },
          { "title": "Atomic Ops", "priority": "medium", "note": "CAS (Compare-And-Swap). Lock-free programming." }
        ]
      },
      {
        "name": "Tools",
        "topics": [
          { "title": "ExecutorService", "priority": "high", "note": "Thread pools. Never manually create threads." },
          { "title": "Concurrent Collections", "priority": "high", "note": "ConcurrentHashMap is asked everywhere." },
          { "title": "Thread-Safety", "priority": "high", "note": "Statelessness is key." },
          { "title": "Immutability", "priority": "high", "note": "Simplest way to achieve thread safety." }
        ]
      }
    ]
  },
  {
    "id": 7,
    "title": "7. Advanced Arch",
    "subtitle": "Technical Depth",
    "groups": [
      {
        "name": "Error Handling",
        "topics": [
          { "title": "Checked vs Unchecked", "priority": "medium", "note": "Compile-time vs Runtime enforcement." },
          { "title": "Custom Exceptions", "priority": "low", "note": "Meaningful error domain modeling." },
          { "title": "Propagation", "priority": "medium", "note": "Bubble up or handle?" },
          { "title": "Optional vs Null", "priority": "medium", "note": "Avoiding NullPointerExceptions." }
        ]
      },
      {
        "name": "Boundaries",
        "topics": [
          { "title": "DTO vs Domain", "priority": "high", "note": "Decoupling API from DB schema." },
          { "title": "Validation Layer", "priority": "medium", "note": "Input sanitization." },
          { "title": "Anti-Corruption Layer", "priority": "low", "note": "Isolating legacy systems." },
          { "title": "Idempotency", "priority": "high", "note": "Retrying safe operations." }
        ]
      },
      {
        "name": "Config",
        "topics": [
          { "title": "Config-Driven Design", "priority": "medium", "note": "Changing behavior without code deploys." },
          { "title": "Feature Toggles", "priority": "medium", "note": "Canary releases and A/B testing." },
          { "title": "Env Separation", "priority": "high", "note": "Dev, Stage, Prod configs." }
        ]
      }
    ]
  },
